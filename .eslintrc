{
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended",
    "plugin:react/recommended",
    "plugin:react-hooks/recommended",
    "plugin:jsx-a11y/recommended"
  ],
  "rules": {
    "prettier/prettier": "warn",
    "semi": "error",
    "quotes": ["error", "single"]
  },
  "plugins": [
    "jsx-a11y",
    "react",
    "react-hooks"
  ]
}
